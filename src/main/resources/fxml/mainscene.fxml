<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<StackPane prefHeight="450.0" prefWidth="600.0" style="-fx-background-color: #115212;" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="at.ac.tuwien.sepm.assignment.individual.restaurant.controller.Mainscenecontroller">
   <children>
      <Button fx:id="btnproductman" mnemonicParsing="false" onAction="#onProductsClickEvent" prefHeight="80.0" prefWidth="104.0" style="-fx-background-color: #E9BC2B, linear-gradient(#FFFF00, #868A08); -fx-background-radius: 10;" text="Products">
         <StackPane.margin>
            <Insets bottom="150.0" right="250.0" />
         </StackPane.margin>
      </Button>
      <Button fx:id="btninvoiceman" mnemonicParsing="false" onAction="#onInvoicesClickEvent" prefHeight="80.0" prefWidth="104.0" style="-fx-background-color: #E9BC2B, linear-gradient(#FFFF00, #868A08); -fx-background-radius: 10;" text="Invoices">
         <StackPane.margin>
            <Insets bottom="150.0" left="250.0" />
         </StackPane.margin>
      </Button>
      <Button fx:id="btnStatistic" mnemonicParsing="false" onAction="#onStatisticClickEvent" prefHeight="80.0" prefWidth="104.0" style="-fx-background-color: #E9BC2B, linear-gradient(#FFFF00, #868A08); -fx-background-radius: 10;" text="Statistic">
         <StackPane.margin>
            <Insets left="250.0" top="150.0" />
         </StackPane.margin>
      </Button>
      <Button fx:id="btnTables" mnemonicParsing="false" onAction="#onTablesClickEvent" prefHeight="80.0" prefWidth="104.0" style="-fx-background-color: #E9BC2B, linear-gradient(#FFFF00, #868A08); -fx-background-radius: 10;" text="Tables">
         <StackPane.margin>
            <Insets right="250.0" top="150.0" />
         </StackPane.margin>
      </Button>
      <Label style="-fx-text-fill: #E8BB2A;" text="The Place to Beer">
         <StackPane.margin>
            <Insets bottom="350.0" />
         </StackPane.margin>
         <font>
            <Font size="22.0" />
         </font>
      </Label>
   </children>
</StackPane>
