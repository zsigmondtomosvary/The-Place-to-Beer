<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.StackPane?>

<StackPane prefHeight="600.0" prefWidth="545.0" style="-fx-background-color: #115212;" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="at.ac.tuwien.sepm.assignment.individual.restaurant.controller.Tablecontroller">
   <children>
        <TableView fx:id="tblorders" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="269.0" prefWidth="254.0">
            <columns>
            <TableColumn fx:id="coltblnr" prefWidth="110.0" text="Tablenumber" />
            <TableColumn fx:id="coldate" prefWidth="143.0" text="Date" />
            </columns>
         <StackPane.margin>
            <Insets bottom="340.0" right="300.0" />
         </StackPane.margin>
        </TableView>
        <Button mnemonicParsing="false" onAction="#onBillClickEvent" prefHeight="40.0" prefWidth="163.0" style="-fx-background-color: #E9BC2B, linear-gradient(#FFFF00, #868A08); -fx-background-radius: 10;" text="Bill">
         <StackPane.margin>
            <Insets left="250.0" />
         </StackPane.margin>
      </Button>
        <Button mnemonicParsing="false" onAction="#onAddClickEvent" prefHeight="40.0" prefWidth="161.0" style="-fx-background-color: #E9BC2B, linear-gradient(#FFFF00, #868A08); -fx-background-radius: 10;" text="Add Product">
         <StackPane.margin>
            <Insets bottom="170.0" left="250.0" />
         </StackPane.margin>
      </Button>
        <Button mnemonicParsing="false" onAction="#onDeleteClickEvent" prefHeight="40.0" prefWidth="158.0" style="-fx-background-color: #E9BC2B, linear-gradient(#FFFF00, #868A08); -fx-background-radius: 10;" text="Delete Product">
         <StackPane.margin>
            <Insets bottom="300.0" left="250.0" />
         </StackPane.margin>
      </Button>
        <Button mnemonicParsing="false" onAction="#onBackClickEvent" prefHeight="40.0" prefWidth="69.0" style="-fx-background-color: #E9BC2B, linear-gradient(#FFFF00, #868A08); -fx-background-radius: 10;" text="Back">
         <StackPane.margin>
            <Insets bottom="500.0" left="350.0" />
         </StackPane.margin>
      </Button>
      <TableView fx:id="tblproducts" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="263.0" prefWidth="543.0">
        <columns>
          <TableColumn fx:id="colname" prefWidth="75.0" text="Name" />
            <TableColumn fx:id="coltext" prefWidth="142.0" text="Description" />
          <TableColumn fx:id="colprice" prefWidth="64.0" text="Price" />
            <TableColumn fx:id="colcat" prefWidth="165.0" text="Category" />
            <TableColumn fx:id="coltax" prefWidth="98.0" text="Tax" />
        </columns>
         <StackPane.margin>
            <Insets top="350.0" />
         </StackPane.margin>
      </TableView>
      <Button mnemonicParsing="false" onAction="#displayOrderProducts" prefHeight="40.0" prefWidth="69.0" style="-fx-background-color: #E9BC2B, linear-gradient(#FFFF00, #868A08); -fx-background-radius: 10;" text="Show">
         <StackPane.margin>
            <Insets right="300.0" />
         </StackPane.margin>
      </Button>
   </children>
</StackPane>
