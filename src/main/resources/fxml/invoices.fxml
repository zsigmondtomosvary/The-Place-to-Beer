<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.StackPane?>

<StackPane prefHeight="500.0" prefWidth="380.0" style="-fx-background-color: #115212;" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="at.ac.tuwien.sepm.assignment.individual.restaurant.controller.Invoicecontroller">
   <children>
      <TableView fx:id="tblinvoices" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="500.0" prefWidth="278.0">
        <columns>
          <TableColumn fx:id="coliid" prefWidth="55.0" text="IID" />
          <TableColumn fx:id="coltablenr" prefWidth="75.0" text="Tablenr" />
            <TableColumn fx:id="colpaiddate" prefWidth="144.0" text="Paid" />
        </columns>
         <StackPane.margin>
            <Insets right="125.0" />
         </StackPane.margin>
      </TableView>
      <Button mnemonicParsing="false" onAction="#onDetailedViewClickEvent" prefHeight="73.0" prefWidth="77.0" style="-fx-background-color: #E9BC2B, linear-gradient(#FFFF00, #868A08); -fx-background-radius: 10;" text="Detailed View" textAlignment="CENTER" wrapText="true">
         <StackPane.margin>
            <Insets left="250.0" />
         </StackPane.margin>
      </Button>
      <Button mnemonicParsing="false" onAction="#onBackClickEvent" style="-fx-background-color: #E9BC2B, linear-gradient(#FFFF00, #868A08); -fx-background-radius: 10;" text="Back">
         <StackPane.margin>
            <Insets bottom="200.0" left="250.0" />
         </StackPane.margin>
      </Button>
   </children>
</StackPane>
